{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "capability-fs-player",
  "description": "Limit fs to $APPDATA/player/*",
  "permissions": [
    "fs:allow-read",
    "fs:allow-write",
    "fs:allow-create",
    "fs:allow-exists",
    "fs:allow-copy-file"
  ],
  "fs": {
    "allow": [
      "$APPDATA/player/*"
    ]
  }

}